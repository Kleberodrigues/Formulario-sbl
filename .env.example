##############################################
# SBL Onboarding Form - .env.example
# Copie este arquivo para `.env` ou `.env.local`
# e preencha com os valores do seu ambiente.
##############################################

# App
VITE_APP_NAME=SBL Onboarding Form
VITE_APP_ENV=development
VITE_APP_URL=http://localhost:5173

# Supabase (Público - pode estar no cliente)
VITE_SUPABASE_URL=https://seu-project.supabase.co
VITE_SUPABASE_ANON_KEY=sua-chave-anonima

# n8n Webhook (Público)
VITE_N8N_WEBHOOK_URL=https://seu-n8n.com/webhook/formulario-sbl

# Mapbox (Opcional; exigido apenas se usar mapa)
# Obtenha o token em: https://account.mapbox.com
VITE_MAPBOX_ACCESS_TOKEN=

# Flags de funcionalidade
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ABANDONMENT_TRACKING=true
VITE_ENABLE_AUTO_SAVE=true
VITE_ENABLE_DEBUG_LOGS=false

# Storage/Uploads
VITE_STORAGE_BUCKET=form-documents
VITE_MAX_FILE_SIZE=10485760    # 10 MB
VITE_MAX_IMAGE_SIZE=5242880    # 5 MB

# Fim do arquivo

